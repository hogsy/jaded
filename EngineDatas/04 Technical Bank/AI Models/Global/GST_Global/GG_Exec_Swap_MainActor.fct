
if (IO_KeyJustPressed(VK_NUMPAD0) || !i_frame_nbr )
{
	if ( i_frame_nbr)
		@"univ" i_jack = vrai - @"univ" i_jack
	if (!@"univ" i_jack)
	{
		@get_jack OBJ_Reinit(vrai)
		@get_kong OBJ_Destroy()
		@get_jack AI_MainActorSet(0)
	}
	else
	{
		@get_jack OBJ_Destroy()
		@get_kong OBJ_Reinit(vrai)
		@get_kong AI_MainActorSet(0)
	}
}
