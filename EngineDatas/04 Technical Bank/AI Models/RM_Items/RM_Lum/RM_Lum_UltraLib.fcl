procedure_ultra void Proc_LUMS_CageSet(object to_cage)
{
	o_cage = to_cage
	if( o_cage )
		i_cage_flag_memo = vrai
}

procedure_ultra void Proc_LUMS_MedaillonSet(object to_gao, vector tv_pos)
{
	i_medaillon_flag = vrai
	v_medaillon_pos = tv_pos
	o_medaillon = to_gao
	OBJ_FlagsControlSet(OBJ_C_ControlFlag_AlwaysActive, none)			// ne passe pas en pause
}
