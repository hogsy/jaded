//---------------------------------------------------------------------------------------------------------------------------------------------------
//  RequestPost prototype + encapsulation
//---------------------------------------------------------------------------------------------------------------------------------------------------
//procedure messageid SPEECH_RequestPost
//(
//	object	argo_Sender, 
//	object 	argo_Target, 
//	int 		argi_TxgKey, 
//	int 		argi_EntryKey, 
//	float 		argf_TimeOut, 
//	int 		argi_Priority, 
//	int 		argi_RandomSize,
//	int			argi_ListId,
//	int			argi_ListIndex,
//	int			argi_Flags
//)
//

#define SPEECH_M_RqHuman(__perso_id, __text_id)	 SPEECH_RequestPost\
	(\
		OBJ_Me(), \
		nobody, \
		__perso_id,\
		__text_id,\
		SPEECH_Cte_TimeOutDefault,\
		SPEECH_Cte_PriorityDefault,\
		SPEECH_Cte_RandomSize,\
		0,\
		0,\
		SPEECH_Cte_FlagsUseHumanId\
	)

#define SPEECH_M_RqHumanTimeOut(__perso_id, __text_id, __timeout)	 SPEECH_RequestPost\
	(\
		OBJ_Me(), \
		nobody, \
		__perso_id,\
		__text_id,\
		__timeout,\
		SPEECH_Cte_PriorityDefault,\
		SPEECH_Cte_RandomSize,\
		0,\
		0,\
		SPEECH_Cte_FlagsUseHumanId\
	)

#define SPEECH_M_RqOneText(__text)	 SPEECH_RequestPost\
	(\
		OBJ_Me(), \
		nobody, \
		TEXT_FileGet(__text),\
		TEXT_EntryGet(__text),\
		SPEECH_Cte_TimeOutDefault,\
		SPEECH_Cte_PriorityDefault,\
		0,\
		0,\
		0,\
		0\
	)

#define SPEECH_M_RqOneText2Params(__txg, __entrykey)	 SPEECH_RequestPost\
	(\
		OBJ_Me(), \
		nobody, \
		__txg,\
		__entrykey,\
		SPEECH_Cte_TimeOutDefault,\
		SPEECH_Cte_PriorityDefault,\
		0,\
		0,\
		0,\
		0\
	)


