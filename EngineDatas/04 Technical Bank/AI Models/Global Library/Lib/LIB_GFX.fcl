// =======================================================================
procedure void LIBGFX_BaveRaptor
(
byrefarr int allid,
byrefarr float alltime, 
byrefarr vector allpos, 
byrefarr float alllen1, 
int cangener
)
{
	object 	to_obj
	vector	tv_temp
