		GFX_Setf(i_gfx2, 13003, 0.5)														// Time fase 1
		GFX_Setf(i_gfx2, 13004, 0.5)														// Time fase 2
		
		GFX_Seti(i_gfx2, 13107, 0)														// Sprites non triés
		GFX_FlagSet(i_gfx2, 0 , 1)
		
		GFX_FlagSet(i_gfx2, 2 , 1)
			
