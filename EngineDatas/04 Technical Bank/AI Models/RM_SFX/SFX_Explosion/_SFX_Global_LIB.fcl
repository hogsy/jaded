	GFX_Setf(i_GFX_Etincelles, 9008, 0.02)																	// Temps entre la génération de 2 étincelles
	GFX_Setf(i_GFX_Etincelles, 9009, 0.2)																	// Temps de mort minimum
	GFX_Setf(i_GFX_Etincelles, 9010, 0.2)																	// Temps de mort maximum

	
