procedure void PROC_SFX_FUMEE_PRESSION(vector tv_pos, float tf_taille, color tc_fume, byref int i_GFX_Fumee)
{
	float		tf_distance
	
	i_GFX_Fumee= GFX_Add(13)
	GFX_MaterialSet(i_GFX_Fumee, get_SFX_light_and_smoke, -1)
