vector	tv_pos, tv_visee
//float	tf_coeff

f_coeff += TIME_GetDt() * des_delai
if	(f_coeff <= 1)
{
	tv_pos = MATH_VecBlend(pos_dep, pos_fin, f_coeff)
	tv_visee = MATH_VecBlend(visee_dep, visee_fin, f_coeff)
	@des_o_perso_recale OBJ_PosSet(tv_pos)
	@des_o_perso_recale OBJ_SightSet(tv_visee)
}
else
{
	@des_o_perso_recale OBJ_PosSet(pos_fin)
	@des_o_perso_recale OBJ_SightSet(visee_fin)
	OBJ_Destroy()
}
