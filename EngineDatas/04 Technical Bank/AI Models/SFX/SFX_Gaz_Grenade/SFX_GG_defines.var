// ETAT
#define ETAT_Launch											00
#define ETAT_Explode											01

// TRACK
#define Ci_Track_Etat											00

#define macro_change_etat(etat)																				\
{																															\
	if (AI_TrackCurGetNum() == Ci_Track_Etat)																\
		AI_TrackCurChangeNow(etat)																				\
	else																													\
		AI_TrackChange(Ci_Track_Etat, etat)																	\
	return																													\
}
