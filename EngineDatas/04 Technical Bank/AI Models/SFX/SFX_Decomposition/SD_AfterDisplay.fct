
int	ti_Mat, ti_Layer
int	ti_layer_id 

//i_dbg_cpt--

for (ti_Mat=0 ; ti_Mat < i_SubMatNbr ; ti_Mat++)
{
	for (ti_Layer=0 ; ti_Layer < 3 ; ti_Layer++)
	{
		ti_layer_id = Af_MyAlpha[ti_Mat][ti_Layer]
		if (ti_layer_id != -1)
		{
			if (	!ti_Layer )
				@o_os MAT_MT_FlagSet(ti_Mat, ti_layer_id, MaterialFlags_ActiveLayer, none) 

			@o_os MAT_MT_SetAlphaTresh(ti_Mat, ti_layer_id, Af_Alpha[ti_Mat][ti_Layer] )
		}
	}
}