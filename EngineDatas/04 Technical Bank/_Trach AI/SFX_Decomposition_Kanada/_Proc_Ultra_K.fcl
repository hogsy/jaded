procedure_ultra void Proc_Decomposition_Init_K(object destinataire, float duree, int couche_viande, int couche_peau1, int couche_peau2)
{
	o_Target = destinataire
	f_Duration = duree
	Af_MyAlpha[i_SubMatNbr][0] = couche_viande
	Af_MyAlpha[i_SubMatNbr][1] = couche_peau1
	Af_MyAlpha[i_SubMatNbr][2] = couche_peau2
	i_SubMatNbr ++
}