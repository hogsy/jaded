
float		tf_dist

if (!AI_TriggerIsValid(trig_lance) || (AI_TriggerIsValid(trig_lance) && call_trigger(trig_lance)) )
{
	DYN_On()
	DYN_FlagsSet(DYN_C_BasicForces,none)
	DYN_GravitySet(Cv_NormalGravity * f_gravity_coef)
	
	tf_dist = MATH_VecDistance(@o_target OBJ_PosGet(), OBJ_PosGet())
	
	DYN_Impulsion(@o_target OBJ_PosGet(), MATH_FloatMax(0, (0.1 + ( tf_dist / 20)) * f_coef_courbe))
	
	AI_TrackCurChange("pc_go")	
}