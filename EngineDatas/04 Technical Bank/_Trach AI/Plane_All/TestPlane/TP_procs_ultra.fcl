procedure_ultra void TP_SetEnemy(object o)
{
	float f
	f=MATH_VecDotProduct(OBJ_SightGet(),MATH_VecNormalize(@o OBJ_PosGet()-OBJ_PosGet()))
	if(!ClosestEnemy || ClosestEnemy==o || ClosestDistance<f)
	{
		ClosestEnemy=o
		ClosestDistance=f
	}
}

procedure_ultra void TP_ResetEnemy(object o)
{
	if(ClosestEnemy==o) ClosestEnemy=nobody
}

procedure_ultra int TP_GetNbProjo()
{
	return NbProjo
}