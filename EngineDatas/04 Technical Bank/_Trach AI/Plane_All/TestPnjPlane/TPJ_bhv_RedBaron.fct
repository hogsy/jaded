object o
vector v
dt=TIME_GetDt()
o=VIEW_GetObject(0)
v=MATH_VecBlend(OBJ_PosGet(),MATH_VecBlend(@o OBJ_PosGet(),@Jack OBJ_PosGet(),0.5),dt*20)
v+=OBJ_HorizonGet()*1.2
v+=OBJ_BankingGet()*0
OBJ_PosSet(v)
OBJ_BankingSet(MATH_VecBlend(OBJ_BankingGet(),@Jack OBJ_BankingGet(),dt*5))
OBJ_SightSet(MATH_VecBlend(OBJ_SightGet(),@o OBJ_SightGet(),dt*5))