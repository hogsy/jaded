#include "PluieDeLances_defines.var"

message 	tm_trigger 

if (AI_TriggerIsValid(tr_attaque))
{
	tm_trigger = AI_TriggerGetMsg(tr_attaque)
			
	if (call_trigger( tr_attaque) )
	{
		if (o_cible != tm_trigger.msg_gao5)
		{
			o_cible = tm_trigger.msg_gao5
		}

		AI_TrackChange(2, "PluieDeLances_etat_attaque")
	}
	else 
	{
		AI_TrackChange(2, "PluieDeLances_etat_attente")
	}
}	


AI_Execute("PluieDeLances_exec_gere_custom_auto_paf")

AI_Execute("PluieDeLances_exec_gere_son")

