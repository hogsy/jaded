float				tf_dist
object			to_gao

if (i_duplicate == 2)
	return

if (o_attaque)
	to_gao = o_attaque
else if (o_attaque_object)
	to_gao = o_attaque_object
else if (i_attaque_id>=0)
	to_gao = @"univ" ao_AllHumains[i_attaque_id]
else
	to_gao = AI_MainActorGet(i_MainActor)

tf_dist = MATH_VecDistance(OBJ_PosGet(),@to_gao OBJ_PosGet())
if (tf_dist < f_dist_attaque_suivre)
{
	o_attaque = to_gao
	AI_TrackCurChangeNow("ci_3_attaque_suivre")
}

if ( ( tf_dist < f_bullrush_distmax ) && ( tf_dist > f_bullrush_distmin ) )
{
	o_attaque = to_gao
	f_bullrush_distance = tf_dist
	AI_TrackCurChangeNow("ci_3_attaque_bullrush")
}
