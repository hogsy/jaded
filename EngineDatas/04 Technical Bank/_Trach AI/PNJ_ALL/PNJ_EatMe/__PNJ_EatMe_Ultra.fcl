procedure_ultra int PNJ_EatMe_GetBidocheType()
{
	return i_bidoche_type
}

procedure_ultra void PNJ_EatMe_SetPoisonned(int ti_on)
{
	i_Poisonned_Type = ti_on
	if( ti_on )
		OBJ_CapaSet(CAPA_Bidoche_Poisonned, faux)
	else
		OBJ_CapaSet(faux, CAPA_Bidoche_Poisonned)
}

procedure_ultra int PNJ_EatMe_IsPoisonned()
{
	return i_Poisonned_Type
}
