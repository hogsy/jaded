//--------( procedure : return 1 if  given perso is attackable )--------
procedure_local int SCS_fct_IsAttackable( object o_cible )
{
	float		tf_temp
	
	if ( @o_cible OBJ_HierarchyGet() )
		return 0
	
	if (o_territoire && !@o_territoire COL_BV_PivotCollide(o_cible))
		return 0
		
//	if ( SCS_fct_IsInWater( @o_cible OBJ_PosGet(), o_cible) == 0 )
	if( ! IsInWater(@o_cible OBJ_PosGet(), tf_temp) )
		return 0
	
	return 1
}

