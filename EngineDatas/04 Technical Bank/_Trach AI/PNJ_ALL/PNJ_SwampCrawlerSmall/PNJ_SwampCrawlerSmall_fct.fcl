procedure_local int SCS_Check_Presence_Interet_Statut( object po_actor, int ti_nbr) 
{
	int					ti_rank
	message		tm_filter
	messageid		tmid_interet_ID
	object			to_gao
	int					ti_reel_nbr
	
	ti_rank = -1
	ti_reel_nbr = 0
	MSG_SetNull(tm_filter)
	tm_filter.msg_gao1 = po_actor
	
	for (	tmid_interet_ID = MSG_GlobalSearchIntGao(C_EVENT_TYPE_Interet, &ti_rank, tm_filter);
			MSG_GlobalIsValid(tmid_interet_ID);
			tmid_interet_ID = MSG_GlobalSearchIntGao(C_EVENT_TYPE_Interet, &ti_rank, tm_filter) )
	{
		to_gao = EVENT_PereGet(tmid_interet_ID)
		if(  to_gao != OBJ_Me() )
		{
			if ( @to_gao AI_IsModel(get_PNJ_SwampCrawlerSmall_path) )
			{
				ti_reel_nbr ++
				if (ti_reel_nbr >= ti_nbr)
					return vrai
			}
		}
	}
	return faux
}


procedure_local void SCS_Compute_All_Z()
{
	GFX_traceZ = move_f_Zsurface + 0.01
	
	move_f_Zsurface += 0.15 * move_f_zoom
	
	move_f_Zsurface -= move_f_zoom
	move_f_Zunderwater = move_f_Zsurface - f_WaterDepth
}

