#include "Interactive_Door_Denham_Switch_defs.var"

float		tf_ratio
vector	tv_pos

f_Timer += TIME_GetDt()
if (f_Timer > TIMER_ACTIVE)
	tf_ratio = 1
else
	tf_ratio = f_Timer / TIMER_ACTIVE
	
tv_pos = v_PosStart + (tf_ratio * (v_PosActive - v_PosStart))
OBJ_PosSet( tv_pos )

if (tf_ratio == 1)
{
	f_Timer = 0
	OBJ_CapaSet( OBJ_Capa_1, 0)
	OBJ_FlagInactiveSet( 1 )
}