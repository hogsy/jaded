#include "Interactive_Door_Denham_Switch_defs.var"

float		tf_ratio
vector	tv_pos

f_Timer += TIME_GetDt()
if (f_Timer > TIMER_DISAPPEAR)
	tf_ratio = 1
else
	tf_ratio = f_Timer / TIMER_DISAPPEAR
	
tv_pos = v_PosStart + (tf_ratio * (v_PosOff - v_PosStart))
OBJ_PosSet( tv_pos )

if (tf_ratio == 1)
{
	f_Timer = 0
	AI_TrackCurChange( "InterDDS_Appear" )
	OBJ_FlagInactiveSet( 1 )
}