procedure void CIM_PrepareMSGplans(int Numplan, int typeduplan, object GAO_1, object GAO_2, float duree, int flags)
{
	message m_msg_out
	MSG_SetNull(m_msg_out)
	m_msg_out.msg_int1 = Numplan
	m_msg_out.msg_int2 = typeduplan
	m_msg_out.msg_int3 = flags
	m_msg_out.msg_sender = GAO_1
	m_msg_out.msg_gao1 = GAO_2
	m_msg_out.msg_vec1 = cvector(duree, 0, 0)
	@get_insert_director MSG_Send(m_msg_out)
}																														
