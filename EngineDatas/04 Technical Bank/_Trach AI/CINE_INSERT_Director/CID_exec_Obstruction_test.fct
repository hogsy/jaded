object	to_collision, to_tete
object	to_mainactor
vector	tv_dep_raytrace, tv_cible_raytrace

to_mainactor = AI_MainActorGet(i_MainActor)

		// Acteur_Cible : on va récupérer l'os de la tête
		to_tete = @o_acteur_cible1 ANI_CanalObjectGet(Anim_Canal_Tete)
		if	( ! to_tete)
		{
			to_tete = o_acteur_cible1
			tv_cible_raytrace = @to_tete OBJ_PosGet() + cvector(0, 0, 0.5)
		}
		else
		{	tv_cible_raytrace = @to_tete OBJ_PosGet()	}	// + cvector(0, 0, 0.5)
		if	(@o_acteur_cam1 OBJ_FlagsIdentityTest(OBJ_C_IdentityFlag_Anims))
		{	tv_dep_raytrace = @o_canal_cam OBJ_PosGet()	}
		else
		{	tv_dep_raytrace = @o_canal_cam OBJ_PosGet() + cvector(0, 0, 0.6)	}

		to_collision = COL_RayObject_Vector(tv_dep_raytrace, tv_cible_raytrace - tv_dep_raytrace, all, OBJ_C_IdentityFlag_Anims, Ci_Filter_IdentityFlag, COL_C_Ray_on_ColMap_NoCrossable)
//		to_collision = COL_RayObject_Vector(v_POS_CAM, v_POS_CIBLE, all, OBJ_C_IdentityFlag_Anims, Ci_Filter_IdentityFlag, COL_C_Ray_on_ColMap_NoCrossable)
		// Il y a un obstacle entre JACK / ANN et l'autre acteur, je change le plan en Grosplan sur le visage de JACK / ANN
		if	( ( ( (to_collision)
			&& ( ! i_InCut) )
			&& ( ! @to_collision OBJ_FlagsIdentityTest(OBJ_C_IdentityFlag_Anims) ) ) )
		{
			MSG_Clear()
			OBJ_CapaSet(0, OBJ_Capa_8)
			i_index_courant = 0
			// Finalement je vais laisser tomber
			returntrack
			// Old version, remplacement par un plan sur le personnage "caché"
			if	((des_o_ANN) && ( (o_acteur_cible1 == des_o_ANN) || (o_acteur_cam1 == des_o_ANN) ) )
			{	o_acteur_cible1 = des_o_ANN	}
			else	if	( (o_acteur_cible1 == to_mainactor) || (o_acteur_cam1 == to_mainactor) )
			{	o_acteur_cible1 = to_mainactor	}
			i_InCut = vrai
			CIM_PrepareMSGplans(0, F_MoyPlan_Aerien, o_acteur_cam1, nobody, 0.5, F_flag_Cible_mouvante)
			OBJ_CapaSet(0, OBJ_Capa_8)
			AI_TrackCurChangeNow("CID_loop")
		}
