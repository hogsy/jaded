#define macro_PrepareMSGplans(typeduplan, GAO_1, GAO_2, duree, cible_mouvante, oriente_GAO_1)	\
{																														\
	m_msg_out.msg_int1 ++																					\
	m_msg_out.msg_int2 = typeduplan																		\
	m_msg_out.msg_int3 = cible_mouvante + oriente_GAO_1										\
	m_msg_out.msg_sender = GAO_1																		\
	m_msg_out.msg_gao1 = GAO_2																		\
	m_msg_out.msg_vec1 = cvector(duree, 0, 0)														\
}																														
