#include "KNG_Defines.var"

int ti_i

for (ti_i = 0; ti_i < i_number_of_natives; ti_i++)
{
	//JIMI_NOTE: even if these lists should always be filled from the top, I put this check in as I encountered a crash...so it CAN happen :)
	if ( ao_native_list[ti_i] )
	{
		@ao_native_list[ti_i] OBJ_Destroy()
	}
	ao_native_list[ti_i] = nobody
}
	 
